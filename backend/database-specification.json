{
    "highscore": {
        "username": {
            "type": "String",
            "required": true
        },
        "score": {
            "type": "Number",
            "required": true
        }
    },

    "user": {
        "username": {
            "type": "String",
            "required": true,
            "unique": true
        },
        "password": {
            "type": "String",
            "required": true
        },
        "company": {
            "type": "String",
            "required": false
        },
        "address": {
            "street": {
                "type": "String",
                "required": false
            },
            "city": {
                "type": "String",
                "required": false
            },
            "zip": {
                "type": "String",
                "required": false
            }
        }
    },

    "highscoreList": {
        "listName": {
            "type": "String",
            "required": true
        },
        "highscores": [
            {
                "username": {
                    "type": "String",
                    "required": true
                },
                "score": {
                    "type": "Number",
                    "required": true
                }
            }
        ]
    },

    "token": {
        "token": {
            "type": "String",
            "required": true
        },
        "username": {
            "type": "String",
            "required": true
        }
    }
}
